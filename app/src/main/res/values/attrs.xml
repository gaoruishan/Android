<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="borderRadius" format="dimension" />
    <attr name="types">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>

    <declare-styleable name="RoundImageView">
        <attr name="borderRadius" />
        <attr name="types" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_widths" format="dimension" />
        <attr name="border_colors" format="color" />
    </declare-styleable>
    <declare-styleable name="BubbleImageView">
        <attr name="bubble_arrowTop" format="dimension|reference" />
        <attr name="bubble_arrowWidth" format="dimension|reference" />
        <attr name="bubble_arrowHeight" format="dimension|reference" />
        <attr name="bubble_arrowOffset" format="dimension|reference" />
        <attr name="bubble_angle" format="dimension|reference" />
        <attr name="bubble_arrowLocation" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ClipView">
        <attr name="clip_length" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MeetingMemItemView">
        <attr name="LayoutType" format="enum" >
            <enum name="LAYOUT_TYPE_TTOP_IBOTTOM" value="0"/>
            <enum name="LAYOUT_TYPE_ILEFT_TRIGHT" value="1" />
            <enum name="LAYOUT_TYPE_TLEFT_IRIGHT" value="2" />
            <enum name="LAYOUT_TYPE_ITOP_TBOTTOM" value="3" />
            <enum name="LAYOUT_TYPE_TGONE" value="4" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CircularProgressBar">
        <attr name="cpbStyle" format="reference" />
        <attr name="cpb_color" format="color" />
        <attr name="cpb_colors" format="reference" />
        <attr name="cpb_stroke_width" format="dimension" />
        <attr name="cpb_min_sweep_angle" format="integer" />
        <attr name="cpb_max_sweep_angle" format="integer" />
        <attr name="cpb_sweep_speed" format="float" />
        <attr name="cpb_rotation_speed" format="float" />
    </declare-styleable>
</resources>